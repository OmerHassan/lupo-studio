package com.dehats.lupo.view
{
	import mx.controls.ProgressBar;

	public class MyProgressBar extends ProgressBar
	{
		public function MyProgressBar()
		{
			super();
		}
		
		public function set progressValue(pValue:Number):void
		{
			setProgress(pValue, 100);						
		}

		public function get progressValue():Number
		{
			return value;
		}
		
	}
}